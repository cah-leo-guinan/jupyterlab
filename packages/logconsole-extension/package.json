{
  "name": "@jupyterlab/logconsole-extension",
  "version": "1.1.0-alpha.0",
  "description": "JupyterLab - Log Console Extension",
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "sideEffects": [
    "style/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build",
    "watch": "tsc -w --listEmittedFiles"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.1.0-alpha.0",
    "@jupyterlab/apputils": "^2.1.0-alpha.0",
    "@jupyterlab/coreutils": "^4.1.0-alpha.0",
    "@jupyterlab/logconsole": "^1.1.0-alpha.0",
    "@jupyterlab/mainmenu": "^2.1.0-alpha.0",
    "@jupyterlab/notebook": "^2.1.0-alpha.0",
    "@jupyterlab/rendermime": "^2.1.0-alpha.0",
    "@jupyterlab/statusbar": "^2.1.0-alpha.0",
    "@phosphor/signaling": "^1.3.0",
    "react": "~16.8.4"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typescript": "~3.5.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
